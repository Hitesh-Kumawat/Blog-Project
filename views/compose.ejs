<%- include('partials/header'); -%>

<h1>Compose</h1>

<form action="/compose" method="post">
  <div class="form-group">
    <label for="title-id">Title</label>
    <input type="text" class="form-control" name="postTitle" id="title-id">
  </div>
  <div class="form-group">
     <label for="post-id">Post</label>
     <textarea class="form-control" id="post-id" name="postBody" rows="5"></textarea>
   </div>
  <button type="submit"class="btn btn-primary" name="button">Publish</button>
</form>

<%- include('partials/footer'); -%>
